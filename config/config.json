    "merchant_fe": {"service_id": 3},
    "merchant_be": {"service_id": 4}
  },
  "environments": {
    "default": {"environment_id": 0},
    "prod": {"environment_id": 1},
    "dev": {"environment_id": 2},
    "stage": {"environment_id": 3},
    "sandbox": {"environment_id": 4}
  },
  "time_interval": 1
}